{
  "name": "glocalcoop/anp-network-main-redux-child",
  "type": "wordpress-theme",
  "license": "MIT",
  "description": "A child theme for the main site of an ANP network, based on Sage with Hybrid Core and Bootstrap.",
  "homepage": "https://github.com/glocalcoop/anp-network-main-redux-child",
  "authors": [
    {
      "name": "Pea, Glocal",
      "email": "pea@glocal.coop",
      "homepage": "https://github.com/misfist"
    }
  ],
  "keywords": [
    "wordpress"
  ],
  "repositories": [
    {
      "type": "package",
      "package": {
          "name": "justintadlock/hybrid-core",
          "version": "master",
          "type": "theme-library",
          "dist": {
              "type": "zip",
              "url": "https://github.com/justintadlock/hybrid-core/archive/master.zip"
          }
      },
      "require": {
        "composer/installers": "~1.0",
      }
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "composer/installers": "~1.0",
    "justintadlock/hybrid-core": "dev-master"
  },
    "extra": {
        "installer-paths": {
            "./libs/{$name}/": ["type:theme-library"]
        }
    }
}